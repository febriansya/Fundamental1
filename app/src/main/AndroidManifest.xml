<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fundamental1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fundamental1">
        <activity
            android:name=".ImplicitActivity"
            android:exported="false"
            android:label="@string/title_activity_implicit"
            android:theme="@style/Theme.Fundamental1.NoActionBar" />
        <activity
            android:name=".OnlyMoveActivity"
            android:exported="false" />
        <activity
            android:name=".MoveWithDataActivity"
            android:exported="false" />
        <activity
            android:name=".MoveActivity"
            android:exported="false"
            android:label="@string/title_activity_move"
            android:theme="@style/Theme.Fundamental1.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>